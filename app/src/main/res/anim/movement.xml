        android:fromXDelta="0%p"
        android:fromYDelta="0%p"
        android:toXDelta="40%p"
        android:toYDelta="-10%p"
        android:duration="1000"
        android:startOffset="1000"
        />
    <translate
        android:fromXDelta="0%p"
        android:fromYDelta="0%p"
        android:toXDelta="-20%p"
        android:toYDelta="30%p"
        android:duration="1000"
        android:startOffset="2000"
        />
